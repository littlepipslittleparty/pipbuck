{
  "name": "pipbuck",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build_and_check": "run-p type-check \"build-only {@}\" --",
    "_build_and_check": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "_serve": "vue-cli-service serve",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build": "vite build",
    "_build": "vue-cli-service build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --quiet",
    "_lint": "vue-cli-service lint",
    "format": "prettier --write src/",
    "fix": "eslint . --fix",
    "_fix": "vue-cli-service lint --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@vue/composition-api": "^1.7.2",
    "core-js": "^3.6.5",
    "howler": "^2.2.3",
    "mapbox-gl": "^3.1.2",
    "pinia": "^2.2.4",
    "register-service-worker": "^1.7.1",
    "screenfull": "^6.0.0",
    "shake.js": "^1.2.2",
    "vue": "^3.5.12",
    "vue-gtag": "^2.0.1",
    "vue-meta": "^3.0.0-alpha.2",
    "vue-router": "^4.4.5",
    "vuex": "^4.0.0-0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-vue": "^9.29.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^9.5.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "sass": "^1.69.5",
    "sass-loader": "^8.0.2",
    "typescript": "~5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-open-graph": "^2.0.3",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vite-svg-loader": "^3.1.1",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint . --quiet",
      "git add"
    ]
  }
}
