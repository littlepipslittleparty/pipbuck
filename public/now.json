{
    "version": 2,
    "name": "pipbuck",
    "public": true,
    "builds": [
        {"src": "**", "use": "@now/static"}
    ],
    "routes": [
      {"src": "^/audio/(.*)", "dest": "/audio/$1"},
      {"src": "^/fonts/(.*)", "dest": "/fonts/$1"},
      {"src": "^/icons/(.*)", "dest": "/icons/$1"},
      {"src": "^/js/(.*)", "dest": "/js/$1"},
      {"src": "^/css/(.*)", "dest": "/css/$1"},
      {"src": "^/img/(.*)", "dest": "/img/$1"},
      {"src": ".*", "dest": "/index.html"}
    ]
}
